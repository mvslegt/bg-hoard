{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "main"
  },
  "npmScope": "bg-hoard",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "store": {
      "tags": ["scope:store", "type:app"]
    },
    "store-e2e": {
      "tags": ["scope:store", "type:e2e"],
      "implicitDependencies": ["store"]
    },
    "store-shared-ui": {
      "tags": ["scope:store", "type:ui"]
    },
    "store-util-formatters": {
      "tags": ["scope:store", "type:util"]
    },
    "store-feature-game-detail": {
      "tags": ["scope:store", "type:feature"]
    },
    "api": {
      "tags": ["scope:api", "type:app"]
    },
    "util-interface": {
      "tags": ["scope:shared", "type:util"]
    },
    "store-shared-ui-e2e": {
      "tags": ["scope:store", "type:e2e"],
      "implicitDependencies": ["store-shared-ui"]
    },
    "api-util-notifications": {
      "tags": ["scope:api", "type:util"]
    }
  }
}
